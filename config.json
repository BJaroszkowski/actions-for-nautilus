{
	"items": [
		{
			"type": "item",
			"label": "Run in node",
			"mimetypes": [
				"application/javascript"
			],
			"command_line": "gnome-terminal -t \"Running Node %b\" --profile \"No Close\" -- node %b",
			"cwd": "%d",
			"max_items": 1
		},
		{
			"type": "menu",
			"label": "Folder Actions",
			"items": [
				{
					"type": "item",
					"label": "Clone GIT Repo Here",
					"mimetypes": [
						"inode/directory"
					],
					"command_line": "zenity-git-clone.sh %f",
					"use_shell": true,
					"max_items": 1
				},
				{
					"type": "item",
					"label": "Execute Command Here",
					"mimetypes": [
						"inode/directory"
					],
					"command_line": "zenity-exec-in-dir.sh %f",
					"use_shell": true,
					"max_items": 1
				},
				{
					"type": "item",
					"label": "Start HTTP Server Here",
					"mimetypes": [
						"inode/directory"
					],
					"command_line": "gnome-terminal -t \"HTTP Server on %f\" -- http-server -p 8000",
					"cwd": "%f",
					"max_items": 1
				}
			]
		},		{
		"type": "menu",
		"label": "Copy Details",
		"items": [
			{
				"type": "item",
				"label": "Copy Name",
				"use_shell": true,
				"command_line": "arg1-to-clipboard.sh %B"
			},
			{
				"type": "item",
				"label": "Copy Path",
				"use_shell": true,
				"command_line": "arg1-to-clipboard.sh %F"
			},
			{
				"type": "item",
				"label": "Copy URI",
				"use_shell": true,
				"command_line": "arg1-to-clipboard.sh %U"
			},
			{
				"type": "item",
				"label": "Test all place holders",
				"command_line": "printf '%%s\n' '#b' %b '#B' %B '#c' %c '#d' %d '#D' %D '#f' %f '#F' %F '#h' %h '#m' %m '#M' %M '#n' %n '#o' %o '#O' %O '#p' %p '#s' %s '#u' %u '#U' %U '#w' %w '#W' %W '#x' %x '#X' %X"
			}
		]
		}
	]
}
